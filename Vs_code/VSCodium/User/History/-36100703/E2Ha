program rost;
uses crt;
var a:array[1..25] of integer;
S:real;
kol,i:integer;
begin
clrscr;
writeln('введите рост всех учеников');
for i:=1 to 25 do
read(a[i]);
for i:=1 to 25 do
S:=0;
S:=S+a[i];
S:=S/25
kol:=0;
for i:=1 to 25 do
begin
  if a[i]>S then 
  begin
   kol:=kol+1;
 end;
end;
writeln(kol);
readkey
end.